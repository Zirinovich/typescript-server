    "clean": "rimraf build",
    "prebuild": "npm run clean",
    "build": "webpack --watch",
    "1postbuild": "webpack --config config/webpack/server.webpack.config.js",
    "prestart": "npm run build",
    "start": "npm-run-all --parallel watch:server watch:build",
    "watch:build": "webpack --watch --config config/webpack/server.webpack.config.js",
    "watch:server": "nodemon --delay 1 --inspect build/server.js --watch build",
    "test": "karma start config/test/karma.conf.js",
    "lint": "tslint \"src/**/**.ts*\"",
    "start:prod": "cross-env NODE_ENV=production npm start",
    "nodestart": "node build/server.js",
    "build:prod": "cross-env NODE_ENV=production npm run build"
  },
